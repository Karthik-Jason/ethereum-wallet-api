     hide-footer\n      centered\n      class=\"bootstrap-modal-wide nopadding\"\n      static\n      lazy\n    >\n      <div class=\"modal-contents\">\n        <div class=\"tx-info-container\">\n          <ul>\n            <li>From Address</li>\n            <li>{{ rawTx.from }}</li>\n          </ul>\n          <ul>\n            <li>To Address</li>\n            <li>{{ rawTx.to }}</li>\n          </ul>\n          <ul>\n            <li>Nonce</li>\n            <li>{{ rawTx.nonce }}</li>\n          </ul>\n          <ul>\n            <li>Value</li>\n            <li>{{ rawTx.value }}</li>\n          </ul>\n          <ul>\n            <li>Data</li>\n            <li class=\"data\">{{ rawTx.data }}</li>\n          </ul>\n          <ul>\n            <li>Chain ID</li>\n            <li>{{ rawTx.chainID }}</li>\n          </ul>\n          <ul>\n            <li>Gas Limit</li>\n            <li>{{ rawTx.gasLimit }}</li>\n          </ul>\n          <ul>\n            <li>Gas Price</li>\n            <li>{{ rawTx.gasPrice }}</li>\n          </ul>\n        </div>\n        <expanding-option :hidebottomborder=\"true\" title=\"Signed Transaction\">\n          <div class=\"raw-signed\">\n            {{ signedTx }}\n          </div>\n        </expanding-option>\n        <expanding-option title=\"Raw Transaction\">\n          {{ rawTx }}\n        </expanding-option>\n        <div class=\"button-block-container\">\n          <standard-button :options=\"buttonConfirmAndSend\" />\n        </div>\n      </div>\n    </b-modal>\n  </div>\n</template>\n\n<script>\nimport StandardInput from '@/components/StandardInput';\nimport Standardbutton from '@/components/Buttons/StandardButton';\nimport ExpandingOption from '@/components/ExpandingOption';\n\nimport AddressBlock from '../AddressBlock';\nexport default {\n  name: 'SendOfflineConfirmation',\n  components: {\n    'standard-input': StandardInput,\n    'standard-button': Standardbutton,\n    'expanding-option': ExpandingOption,\n    'address-block':