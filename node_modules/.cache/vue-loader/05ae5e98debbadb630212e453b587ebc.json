fault: () => {}\n    },\n    updateEmail: {\n      type: Function,\n      default: () => {}\n    },\n    updateSocialLinks: {\n      type: Function,\n      default: () => {}\n    },\n    form: {\n      type: Object,\n      default: function() {\n        return {};\n      }\n    },\n    socialAccts: {\n      type: Array,\n      default: function() {\n        return [];\n      }\n    },\n    updateDisableBtn: {\n      type: Function,\n      default: () => {}\n    }\n  },\n  data() {\n    return {\n      socialLinks: [],\n      socialLinksError: false\n    };\n  },\n  methods: {\n    openSocialAcctModal() {\n      this.$refs.socialacct.$refs.socialAcctModal.show();\n      this.socialLinksError = this.socialLinks.length === 0;\n    },\n    addSocialAccount(account) {\n      this.socialAccts.push(account);\n      this.socialLinks.push(account.url);\n      this.socialLinksError = this.socialLinks.length === 0;\n      this.updateSocialLinks(this.socialLinks, this.socialAccts);\n    },\n    removeSocialLink(idx) {\n      this.socialAccts.splice(idx, 1);\n      this.socialLinks.splice(idx, 1);\n      this.socialLinksError = this.socialLinks.length === 0;\n      this.updateSocialLinks(this.socialLinks, this.socialAccts);\n    }\n  }\n};\n",{"version":3,"sources":["AboutYourTeamContainer.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8JA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"AboutYourTeamContainer.vue","sourceRoot":"src/layouts/DappSubmissionLayout/containers/AboutYourTeamContainer","sourcesContent":["<template>\n  <div class=\"about-your-dapp mb-5 mt-5\">\n    <social-acct-modal ref=\"socialacct\" @addSocialAccount=\"addSocialAccount\" />\n    <b-form onsubmit=\"return false;\">\n      <b-form-group>\n        <label class=\"dapp-label\"\n          >Authors\n          <popover\n            :popcontent=\"$t('dappsSubmission.authors')\"\n            class=\"dapp-popover\"\n          ></popover>\n        </label>\n        <div class=\"dapp-input\">\n          <b-form-input\n            v-validate=\"'required'\"\n            id=\"authors\"\n            v-model=\"form.authors\"\n            name=\"authors\"\n            placeholder=\"e.g. James Lee; Emilie Roy; Edward McCormick\"\n            type=\"text\"\n            @update=\"updateAuthors\"\n          >\n          </b-form-input>\n          <span>*</span>\n        </div>\n        <p v-if=\"errors.has('authors')\" class=\"error\">\n          {{ errors.first('authors') }}\n        </p>\n      </b-form-group>\n      <b-form-group>\n        <label class=\"dapp-label\">My full name </label>\n        <div class=\"dapp-input\">\n          <b-form-input\n            v-validate=\"'required'\"\n            id=\"fullName\"\n            v-model=\"form.fullName\"\n            name=\"name\"\n            type=\"text\"\n            @update=\"updateFullName\"\n          >\n          </b-form-input>\n          <span>*</span>\n        </div>\n        <p v-if=\"errors.has('name')\" class=\"error\">\n          {{ errors.first('name') }}\n        </p>\n      </b-form-group>\n      <b-form-group>\n        <label class=\"dapp-label\">My email </label>\n        <div class=\"dapp-input\">\n          <b-form-input\n            v-validate=\"'email|required'\"\n            id=\"emailAddress\"\n            v-model=\"form.email\"\n            name=\"email\"\n            type=\"text\"\n            @update=\"updateEmail\"\n            @change=\"updateDisableBtn(errors)\"\n          >\n          </b-form-input>\n          <span>*</span>\n        </div>\n        <p v-if=\"errors.has('email')\" class=\"error\">\n          {{ errors.first('email') }}\n        </p>\n      </b-form-group>\n      <b-form-group>\n        <div class=\"social-links-container\">\n          <div class=\"social-links-header-wrapper\">\n            <div class=\"social-links-header\">\n              <label class=\"dapp-social-label\"> Company social links</label>\n              <button class=\"add-btn pull-right\" @click=\"openSocialAcctModal\">\n                Add +\n              </button>\n            </div>\n            <span class=\"required-icon\">*</span>\n          </div>\n          <div v-if=\"socialAccts.length > 0\" class=\"social-links-content\">\n            <div\n              v-for=\"(acct, idx) in socialAccts\"\n              :idx=\"idx\"\n              :key=\"acct + idx\"\n              class=\"social-account\"\n            >\n              <div class=\"fake-input mt-2\">\n                <img :src=\"acct.src\" class=\"social-img\" />\n                <span class=\"social-url ml-1\">{{ acct.url }}</span>\n              </div>\n              <img\n                class=\"social-remove\"\n                src=\"@/assets/images/icons/remove.png\"\n                @click=\"removeSocialLink(idx)\"\n              />\n            </div>\n          </div>\n        </div>\n        <p v-if=\"socialLinksError\" class=\"error\">\n          The company social links field is required\n        </p>\n      </b-form-group>\n      <b-form-group>\n        <label class=\"dapp-label\">Company website </label>\n        <div class=\"dapp-input\">\n          <b-form-input\n            v-validate=\"'url:require_protocol'\"\n            id=\"companyWebsite\"\n            v-model=\"form.companyWebsite\"\n            type=\"text\"\n            name=\"website\"\n            placeholder=\"URL link\"\n            @change=\"updateDisableBtn(errors)\"\n          >\n          </b-form-input>\n        </div>\n        <p v-if=\"errors.has('website')\" class=\"error\">\n          {{ errors.first('website') }}\n        </p>\n      </b-form-group>\n      <b-form-group>\n        <label class=\"dapp-label\"\n          >Software license\n          <popover\n            :popcontent=\"$t('dappsSubmission.softwareLicense')\"\n            class=\"dapp-popover\"\n          ></popover>\n        </label>\n        <div class=\"dapp-input\">\n          <b-form-input\n            id=\"softwareLicense\"\n            v-model=\"form.license\"\n            type=\"text\"\n            placeholder=\"(e.g. MIT, GPL, Proprietary)\"\n          >\n          </b-form-input>\n        </div>\n      </b-form-group>\n      <b-form-group>\n        <label class=\"dapp-label\">Additional notes</label>\n        <div class=\"dapp-input\">\n          <b-form-textarea\n            v-validate=\"'max:300'\"\n            v-model=\"form.additionalNotes\"\n            size=\"lg\"\n            rows=\"5\"\n            name=\"notes\"\n            placeholder=\"300 characters\"\n            @change=\"updateDisableBtn(errors)\"\n          ></b-form-textarea>\n        </div>\n        <p v-if=\"errors.has('notes')\" class=\"error\">\n          {{ errors.first('notes') }}\n        </p>\n      </b-form-group>\n    </b-form>\n  </div>\n</template>\n\n<script>\nimport PopOver from '@/components/PopOver';\nimport SocialAcctModal from '../../components/SocialAcctModal';\n\nexport default {\n  components: {\n    popover: PopOver,\n    'social-acct-modal': SocialAcctModal\n  },\n  props: {\n    updateAuthors: {\n      type: Function,\n      default: () => {}\n    },\n    updateFullName: {\n      type: Function,\n      default: () => {}\n    },\n    updateEmail: {\n      type: Function,\n      default: () => {}\n    },\n    updateSocialLinks: {\n      type: Function,\n      default: () => {}\n    },\n    form: {\n      type: Object,\n      default: function() {\n        return {};\n      }\n    },\n    socialAccts: {\n      type: Array,\n      default: function() {\n        return [];\n      }\n    },\n    updateDisableBtn: {\n      type: Function,\n      default: () => {}\n    }\n  },\n  data() {\n    return {\n      socialLinks: [],\n      socialLinksError: false\n    };\n  },\n  methods: {\n    openSocialAcctModal() {\n      this.$refs.socialacct.$refs.socialAcctModal.show();\n      this.socialLinksError = this.socialLinks.length === 0;\n    },\n    addSocialAccount(account) {\n      this.socialAccts.push(account);\n      this.socialLinks.push(account.url);\n      this.socialLinksError = this.socialLinks.length === 0;\n      this.updateSocialLinks(this.socialLinks, this.socialAccts);\n    },\n    removeSocialLink(idx) {\n      this.socialAccts.splice(idx, 1);\n      this.socialLinks.splice(idx, 1);\n      this.socialLinksError = this.socialLinks.length === 0;\n      this.updateSocialLinks(this.socialLinks, this.socialAccts);\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import 'AboutYourTeamContainer.scss';\n@import '../AboutYourDappTeamContainer.scss';\n</style>\n\n<style lang=\"scss\">\n.dapp-social-label {\n  align-items: center;\n  display: flex;\n  font-size: 16px;\n  margin-bottom: 10px;\n  width: auto;\n}\n</style>\n"]}]}