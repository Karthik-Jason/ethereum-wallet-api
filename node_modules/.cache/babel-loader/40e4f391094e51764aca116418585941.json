 function _default() {}\n    }\n  },\n  data: function data() {\n    return {\n      showRaw: false,\n      jsonFile: '',\n      jsonFileName: \"signedTransactionObject-\".concat(+new Date(), \".json\")\n    };\n  },\n  watch: {\n    signedTx: function signedTx(newVal) {\n      var parsedVal = JSON.parse(newVal);\n      var rawTx = this.rawTx;\n      delete rawTx['generateOnly'];\n      var txObj = Object.assign({}, parsedVal.tx, rawTx);\n      delete parsedVal['tx'];\n      parsedVal['tx'] = txObj;\n      var string = JSON.stringify(parsedVal);\n      var blob = new Blob([string], {\n        type: 'mime'\n      });\n      this.jsonFileName = \"signedTransactionObject-\".concat(+new Date(), \".json\");\n      this.jsonFile = window.URL.createObjectURL(blob);\n    }\n  },\n  methods: {\n    copyAndContinue: function copyAndContinue() {\n      this.$refs.signedTxInput.select();\n      document.execCommand('copy');\n      this.$refs.signedTx.hide();\n    }\n  }\n};",{"version":3,"sources":["SignedTxModal.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyDA,eAAA;AACA,EAAA,IAAA,EAAA,eADA;AAEA,EAAA,KAAA,EAAA;AACA,IAAA,QAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA,KADA;AAKA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA,oBAAA;AACA,eAAA,EAAA;AACA;AAJA,KALA;AAWA,IAAA,UAAA,EAAA;AACA,MAAA,IAAA,EAAA,QADA;AAEA,MAAA,OAAA,EAAA,oBAAA,CAAA;AAFA;AAXA,GAFA;AAkBA,EAAA,IAlBA,kBAkBA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,KADA;AAEA,MAAA,QAAA,EAAA,EAFA;AAGA,MAAA,YAAA,oCAAA,CAAA,IAAA,IAAA,EAAA;AAHA,KAAA;AAKA,GAxBA;AAyBA,EAAA,KAAA,EAAA;AACA,IAAA,QADA,oBACA,MADA,EACA;AACA,UAAA,SAAA,GAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA;AACA,aAAA,KAAA,CAAA,cAAA,CAAA;AACA,UAAA,KAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,SAAA,CAAA,EAAA,EAAA,KAAA,CAAA;AACA,aAAA,SAAA,CAAA,IAAA,CAAA;AACA,MAAA,SAAA,CAAA,IAAA,CAAA,GAAA,KAAA;AAEA,UAAA,MAAA,GAAA,IAAA,CAAA,SAAA,CAAA,SAAA,CAAA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,CAAA,MAAA,CAAA,EAAA;AACA,QAAA,IA